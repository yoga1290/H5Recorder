{
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true
    },
    "source": {
        "include": ["ffmpegUtil", "Handlers", "index.js"],
        "exclude": ["doc"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ]
}
